@model MvcOnlineCommercialAutomation.Models.Entities.Category
@{
    ViewBag.Title = "EditCategory";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Kategori Güncelleme</h2>
<br />

<form method="post">
    @Html.HiddenFor(x=> x.CategoryID)
    <label>Kategori Adı</label>
    @Html.TextBoxFor(x => x.CategoryName, new { @class = "form-control" })
    <button class="btn btn-outline-warning">Güncelle</button>
</form>
