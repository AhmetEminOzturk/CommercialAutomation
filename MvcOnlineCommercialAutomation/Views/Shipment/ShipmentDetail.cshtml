@using MvcOnlineCommercialAutomation.Models.Entities
@model List<ShipmentTrack>
@{
    ViewBag.Title = "ShipmentDetail";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<table class="table table-striped">
    <tr>
        <th>Açıklama</th>
        <th>Tarih</th>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.Description</td>
            <td>@x.Date</td>
        </tr>
    }
</table>

