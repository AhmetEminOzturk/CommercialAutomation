@model MvcOnlineCommercialAutomation.Models.Entities.Department
@{
    ViewBag.Title = "UpdateDepartment";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Departman Güncelleme Sayfası</h2>
<br />
<form method="post">
    @Html.HiddenFor(x => x.DepartmentID)
    <label>Departman Adı</label>
    @Html.TextBoxFor(x => x.DepartmentName, new { @class = "form-control" })
    <br />
    <button class="btn btn-outline-success">Kaydet</button>
</form>

